{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@mock/*": ["./src/mock/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@mutation/*": ["./src/graphql/mutation/*"],
      "@query/*": ["./src/graphql/query/*"],
      "@Generated": ["./src/generated/graphql"],
      "@ssr/*": ["./src/graphql/ssr/*"],
      "@Uri/*": ["./src/graphql/uri/*"],
      "@apollo/*": ["./src/apollo/*"],
      "@Services": ["./src/services/index"],
      "@Redux/*": ["./src/redux/*"],
      "@Util/*": ["./src/utils/*"],


    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
